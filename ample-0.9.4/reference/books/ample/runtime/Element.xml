<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="../../../stylesheets/page.xsl"?>
<class name="Element" extends="runtime:Node">
	<abstract>
		<locale>
			Represents an element.
		</locale>
		<locale xml:lang="ru">
			Элемент.
		</locale>
	</abstract>
	<remarks />
	<examples />
	<members>
		<constants />
		<attributes>
			<attribute name="id" readonly="false">
				<description>
					<locale>
						Element identifier.
					</locale>
					<locale xml:lang="ru">
						Позволяет получить или выставить уникальный идентификатор элемента.
					</locale>
				</description>
			</attribute>
			<!--attribute name="style" readonly="false">
				<description>
					<locale>
						Retrieves the inline style declaration.
					</locale>
					<locale xml:lang="ru">
						Позволяет получить или выставить каскадный стиль.
					</locale>
				</description>
			</attribute-->
			<attribute name="on***" readonly="true">
				<description>
					<locale>
						Enables in-line handler for *** event.
					</locale>
				</description>
			</attribute>
			<!--attribute name="class" readonly="false">
				<description>
					<locale>
						Sets or retrieves CSS class name.
					</locale>
					<locale xml:lang="ru">
						Позволяет получить или выставить имя класса, определенного в CSS.
					</locale>
				</description>
			</attribute-->
			<attribute name="xml:base" readonly="true">
				<description>
					<locale>
						Retrieves baseURI for the object.
					</locale>
					<locale xml:lang="ru">
						URI ссылка, которая определяет другой базовый URI отличный от базового URI документа для его фрагмента.
					</locale>
				</description>
			</attribute>
			<attribute name="xml:space" readonly="true" default="default">
				<values>
					<value name="preserve">
						<description>
							<locale>
								All whitespaces will be preserved in the document.
							</locale>
							<locale xml:lang="ru">
								Все пробельные символы сохраняются в объектной модели документа.
							</locale>
						</description>
					</value>
					<value name="default">
						<description>
							<locale>
								Standard processing of whitespaces
							</locale>
							<locale xml:lang="ru">
								Стандартная обработка пробельных символов.
							</locale>
						</description>
					</value>
				</values>
				<description>
					<locale>
						Instructs DOM implementation on weather to preserve whitespace text nodes.
					</locale>
					<locale xml:lang="ru">
						Указывает, будут ли пробелы представлены в объектной модели фрагмента документа.
					</locale>
				</description>
			</attribute>
			<!-- Drag And Drop -->
			<attribute name="aml:draggable" readonly="false" default="false">
				<values>
					<value name="true">
						<description>
							<locale>
								Element can be dragged.
							</locale>
							<locale xml:lang="ru">
								Элемент может перетаскиваться.
							</locale>
						</description>
					</value>
					<value name="false">
						<description>
							<locale>
								Element can not be dragged.
							</locale>
							<locale xml:lang="ru">
								Элемент не может перетаскиваться.
							</locale>
						</description>
					</value>
				</values>
				<description>
					<locale>
						Specifies wether element can be dragged by user.
					</locale>
					<locale xml:lang="ru">
						Указывает, может ли пользователь перетаскивать элемент или нет.
					</locale>
				</description>
			</attribute>
			<attribute name="aml:droppable" readonly="false" default="false">
				<values>
					<value name="true">
						<description>
							<locale>
								Element can be used as drop target.
							</locale>
							<locale xml:lang="ru">
								Элемент является местом оставления перетаскиваемого объекта.
							</locale>
						</description>
					</value>
					<value name="false">
						<description>
							<locale>
								Element can not be used as drop target.
							</locale>
							<locale xml:lang="ru">
								Элемент не является местом оставления перетаскиваемого объекта.
							</locale>
						</description>
					</value>
				</values>
				<description>
					<locale>
						Specifies wether element can be used as drop target.
					</locale>
					<locale xml:lang="ru">
						Указывает, является ли элемент местом оставления перетаскиваемого объекта.
					</locale>
				</description>
			</attribute>
			<!-- Resize -->
			<attribute name="aml:resizable" readonly="false" default="false">
				<values>
					<value name="true">
						<description>
							<locale>
								Elements boundaries can be modified.
							</locale>
							<locale xml:lang="ru">
								Размеры элемента могут быть изменены пользователем.
							</locale>
						</description>
					</value>
					<value name="false">
						<description>
							<locale>
								Elements boundaries can not be modified.
							</locale>
							<locale xml:lang="ru">
								Размеры элемента не изменяемы.
							</locale>
						</description>
					</value>
				</values>
				<description>
					<locale>
						Specifies whether element boundaries can be modified by user.
					</locale>
					<locale xml:lang="ru">
						Указывает, может ли пользователь изменять размеры элемента.
					</locale>
				</description>
			</attribute>

			<attribute name="aml:resize-edges" readonly="false" default="false">
				<description>
					<locale>
						Specifies what edges can be resized. The attribute value is a space separated list of "top", "right", "bottom" and "left" tokens.
					</locale>
				</description>
			</attribute>
			<!-- Selection -->
			<attribute name="aml:selectable" readonly="false" default="inherit">
				<values>
					<value name="true">
						<description>
							<locale>
								Text selection is enabled on element
							</locale>
						</description>
					</value>
					<value name="false">
						<description>
							<locale>
								Text selection is disabled on element
							</locale>
						</description>
					</value>
					<value name="inherit">
						<description>
							<locale>
								Text selection behavior is inherited from its parent container.
							</locale>
						</description>
					</value>
				</values>
				<description>
					<locale>
						Enables/disables text selection on element.
					</locale>
				</description>
			</attribute>
			<!-- Localizaiton -->
			<attribute name="aml:content" type="String">
				<description>
					<locale>
						Name of locale key to substitute element's content.
					</locale>
				</description>
			</attribute>
			<attribute name="aml:values" type="String">
				<description>
					<locale>
						Semicolon-separated list of name:locale key pairs to substitute element's attributes or properties.
						For example: value:key_value;.innerHTML:key_innerHTML
					</locale>
				</description>
			</attribute>
		</attributes>
		<methods>
			<method name="getAttribute" type="String">
				<arguments>
					<argument name="name" type="String" required="true">
						<description>
							<locale>
								Attribute name
							</locale>
							<locale xml:lang="ru">
								Имя атрибута.
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						Retrieves the value of the specified attribute.
					</locale>
					<locale xml:lang="ru">
						Возвращает значение заданного атрибута.
					</locale>
				</description>
			</method>
			<method name="getAttributeNS" type="String">
				<arguments>
					<argument name="namespaceURI" type="String" required="true">
						<description>
							<locale>
								Namespace URI
							</locale>
							<locale xml:lang="ru">
								URI пространства имен атрибута.
							</locale>
						</description>
					</argument>
					<argument name="localName" type="String" required="true">
						<description>
							<locale>
								Attribute name
							</locale>
							<locale xml:lang="ru">
								Имя атрибута.
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						Retrieves the value of the specified attribute from the specified namespace.
					</locale>
					<locale xml:lang="ru">
						Возвращает значение указанного атрибута из указанного пространства имен.
					</locale>
				</description>
			</method>
			<method name="setAttribute" type="Void">
				<arguments>
					<argument name="name" type="String" required="true">
						<description>
							<locale>
								Attribute name
							</locale>
							<locale xml:lang="ru">
								Имя атрибута.
							</locale>
						</description>
					</argument>
					<argument name="value" type="String" required="true">
						<description>
							<locale>
								Attribute value
							</locale>
							<locale xml:lang="ru">
								Значение атрибута.
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						Sets the value of the specified attribute.
					</locale>
					<locale xml:lang="ru">
						Выставляет значение заданного атрибута.
					</locale>
				</description>
			</method>
			<method name="setAttributeNS" type="Void">
				<arguments>
					<argument name="namespaceURI" type="String" required="true">
						<description>
							<locale>
								Namespace URI
							</locale>
							<locale xml:lang="ru">
								URI пространства имен атрибута.
							</locale>
						</description>
					</argument>
					<argument name="name" type="String" required="true">
						<description>
							<locale>
								Attribute name
							</locale>
							<locale xml:lang="ru">
								Имя атрибута.
							</locale>
						</description>
					</argument>
					<argument name="value" type="String" required="true">
						<description>
							<locale>
								Attribute value
							</locale>
							<locale xml:lang="ru">
								Значение атрибута.
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						Sets the value of the specified attribute from the given namespace.
					</locale>
					<locale xml:lang="ru">
						Выставляет значение заданного атрибута из заданного пространства имен.
					</locale>
				</description>
			</method>
			<method name="getElementsByTagName" type="NodeList">
				<arguments>
					<argument name="name" type="String" required="true">
						<description>
							<locale>
								Element name.
							</locale>
							<locale xml:lang="ru">
								Имя элемента.
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						Retrieves a <link type="class">NodeList</link> collection of <link type="class">Element</link> objects based on the specified element name.
					</locale>
					<locale xml:lang="ru">
						Возвращает коллекцию <link type="class">NodeList</link> объектов <link type="class">Element</link> по заданному имени в порядке следования их в документе.
					</locale>
				</description>
			</method>
			<method name="getElementsByTagNameNS" type="NodeList">
				<arguments>
					<argument name="namespaceURI" type="String" required="true">
						<description>
							<locale>
								Namespace URI.
							</locale>
							<locale>
								URI пространства имен элемента.
							</locale>
						</description>
					</argument>
					<argument name="localName" type="String" required="true">
						<description>
							<locale>
								Element name.
							</locale>
							<locale xml:lang="ru">
								Имя элемента.
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						Retrieves a <link type="class">NodeList</link> collection of <link type="class">Element</link> objects based on the specified element name from the given namespace.
					</locale>
					<locale xml:lang="ru">
						Возвращает коллекцию <link type="class">NodeList</link> объектов <link type="class">Element</link> по заданным имени и пространству имен в порядке следования их в документе.
					</locale>
				</description>
			</method>
			<method name="hasAttribute" type="Boolean">
				<arguments>
					<argument name="name" type="String" required="true">
						<description>
							<locale>
								Attribute name.
							</locale>
							<locale xml:lang="ru">
								Имя атрибута.
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						Returns a value that indicates whether the object has specified attribute.
					</locale>
					<locale xml:lang="ru">
						Возвращаемое значение указывает, имеет ли объект <link type="class">Element</link> заданный атрибут или нет.
					</locale>
				</description>
			</method>
			<method name="hasAttributeNS" type="Boolean">
				<arguments>
					<argument name="namespaceURI" type="String" required="true">
						<description>
							<locale>
								Namespace URI.
							</locale>
							<locale xml:lang="ru">
								URI пространства имен атрибута.
							</locale>
						</description>
					</argument>
					<argument name="localName" type="String" required="true">
						<description>
							<locale>
								Attribute name.
							</locale>
							<locale xml:lang="ru">
								Имя атрибута.
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						Returns a value that indicates whether the object has specified attribute from the given namespace.
					</locale>
					<locale xml:lang="ru">
						Возвращаемое значение указывает, имеет ли объект <link type="class">Element</link> заданный атрибут из заданного пространства имен или нет.
					</locale>
				</description>
			</method>
			<method name="removeAttribute" type="Void">
				<arguments>
					<argument name="name" type="String" required="true">
						<description>
							<locale>
								Attribute name.
							</locale>
							<locale xml:lang="ru">
								Имя атрибута.
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						Removes the given attribute from the element.
					</locale>
					<locale xml:lang="ru">
						Удаляет заданный атрибут у элемента.
					</locale>
				</description>
			</method>
			<method name="removeAttributeNS" type="Void">
				<arguments>
					<argument name="namespaceURI" type="String" required="true">
						<description>
							<locale>
								Namespace URI.
							</locale>
							<locale xml:lang="ru">
								URI пространства имен атрибута.
							</locale>
						</description>
					</argument>
					<argument name="name" type="String" required="true">
						<description>
							<locale>
								Attribute name.
							</locale>
							<locale xml:lang="ru">
								Имя атрибута.
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						Removes the given attribute from the given namespace from the element.
					</locale>
					<locale xml:lang="ru">
						Удаляет заданный атрибут из заданного пространства имен у элемента.
					</locale>
				</description>
			</method>
			<!-- DOM-Selectors -->
			<method name="querySelector" type="Element">
				<arguments>
					<argument name="selector" type="String" required="true">
						<description>
							<locale>
								CSS query.
							</locale>
						</description>
					</argument>
					<argument name="NSResolver" type="Function" required="false">
						<description>
							<locale>
								Resolver function used to resolve namespace prefixes used in query.
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						Returns <link type="class">Element</link> that matches CSS query.
					</locale>
					<locale xml:lang="ru">
						Возвращает объект <link type="class">Element</link> по заданному CSS запросу.
					</locale>
				</description>
			</method>
			<method name="querySelectorAll" type="NodeList">
				<arguments>
					<argument name="selector" type="String" required="true">
						<description>
							<locale>
								CSS query.
							</locale>
						</description>
					</argument>
					<argument name="NSResolver" type="Function" required="false">
						<description>
							<locale>
								Resolver function used to resolve namespace prefixes used in query.
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						Returns <link type="class">NodeList</link> of <link type="class">Element</link> that match CSS query.
					</locale>
					<locale xml:lang="ru">
						Возвращает коллекцию <link type="class">NodeList</link> из объектов <link type="class">Element</link> по заданному запросу в порядке, в котором элементы следуют в документе.
					</locale>
				</description>
			</method>
			<!-- Ample methods -->
			<method name="$activate" type="Void">
				<description>
					<locale>
						Activates element by dispatching <link type="event">DOMActivate</link> event to the element.
					</locale>
				</description>
			</method>
			<method name="$mapAttribute" type="Void">
				<arguments>
					<argument name="name" type="String">
						<description>
							<locale>
								Attribute name
							</locale>
						</description>
					</argument>
					<argument name="value" type="String">
						<description>
							<locale>
								Attribute value. null if attribute is being removed.
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						<b>UI element definition API.</b> Provide your function to map attribute value to shadow tree. Not called for id, class and style attributes.
					</locale>
				</description>
			</method>
			<method name="$getContainer" type="HTMLElement">
				<arguments>
					<argument name="pseudoElement" type="String" required="false">
						<description>
							<locale>
								Name of pseudo-element.
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						<b>UI element definition API.</b> Returns a pointer to a (pseudo-) element in the shadow content.
					</locale>
				</description>
			</method>
			<method name="$getTagOpen" type="String">
				<description>
					<locale>
						<b>UI element definition API.</b> Provide your function to return shadow content to be inserted before element children.
					</locale>
				</description>
			</method>
			<method name="$getTagClose" type="String">
				<description>
					<locale>
						<b>UI element definition API.</b> Provide your function to return shadow content to be inserted after element children.
					</locale>
				</description>
			</method>
			<method name="$setPseudoClass" type="Void">
				<arguments>
					<argument name="name" type="String" required="true">
						<description>
							<locale>
								Name of CSS pseudoclass
							</locale>
							<locale xml:lang="ru">
								Имя псевдокласса.
							</locale>
						</description>
					</argument>
					<argument name="state" type="Boolean" required="true">
						<description>
							<locale>
								Flag indicating wether pseudoclass should be set or reset.
							</locale>
							<locale xml:lang="ru">
								Указывает, должен ли псевдокласс быть выставлен или снят.
							</locale>
						</description>
					</argument>
					<argument name="pseudoElement" type="String" required="false">
						<description>
							<locale>
								Name of pseudo-element.
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						<b>UI element definition API.</b> Applies the pseudo class to an element.
					</locale>
					<locale xml:lang="ru">
						Позволяет управлять псевдоклассами элемента.
					</locale>
				</description>
			</method>
			<!--  -->
			<method name="getBoundingClientRect" type="TextRectangle">
				<arguments />
				<description>
					<locale>
						Returns an object with <em>top</em>, <em>right</em>, <em>bottom</em>, <em>left</em> members keeping
						the absolute coordinates of the object in the screen.
					</locale>
				</description>
			</method>
			<method name="scrollIntoView" type="Void">
				<arguments>
					<argument name="top" type="Boolean">
						<description>
							<locale>
								If value is true, scrolls the object so that top of the object is visible at the top of the window.
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						Causes the object to scroll into view, aligning it either at the top or bottom of the window.
					</locale>
				</description>
			</method>
			<!-- Capture -->
			<method name="setCapture" type="Void">
				<arguments>
					<argument name="containerCapture" type="Boolean" required="false">
						<description>
							<locale>
								A Boolean that specifies capture mode.
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						Sets the mouse capture to the object that belongs to the current document.
					</locale>
				</description>
			</method>
			<method name="releaseCapture" type="Void">
				<description>
					<locale>
						Removes mouse capture from the object in the current document.
					</locale>
				</description>
			</method>
			<!-- Focus -->
			<method name="focus" type="Void">
				<arguments />
				<description>
					<locale>
						Moves focus to the element.
					</locale>
					<locale xml:lang="ru">
						Перемещает фокус на элемент.
					</locale>
				</description>
			</method>
			<method name="blur" type="Void">
				<arguments />
				<description>
					<locale>
						Removes focus from the element.
					</locale>
					<locale xml:lang="ru">
						Убирает фокус с элемента.
					</locale>
				</description>
			</method>
			<method name="$isAccessible" type="Boolean">
				<arguments />
				<description>
					<locale>
